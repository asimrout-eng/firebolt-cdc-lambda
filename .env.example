# Firebolt CDC Lambda Configuration
# Copy this to .env and fill in your values

# AWS Configuration
AWS_REGION=ap-south-1
ENVIRONMENT=prod

# S3 Configuration
S3_BUCKET_NAME=fcanalytics
S3_PREFIX=firebolt_dms_job/

# Lambda Configuration
LAMBDA_FUNCTION_NAME=firebolt-cdc-processor
LAMBDA_TIMEOUT=300
LAMBDA_MEMORY_SIZE=512

# ============================================
# FIREBOLT CREDENTIALS (REQUIRED)
# ============================================
FIREBOLT_ACCOUNT=faircent
FIREBOLT_DATABASE=
FIREBOLT_ENGINE=
FIREBOLT_USERNAME=
FIREBOLT_PASSWORD=

# Firebolt External Location
LOCATION_NAME=s3_raw_dms

# Table Keys Configuration
TABLE_KEYS_S3_BUCKET=fcanalytics
TABLE_KEYS_S3_KEY=firebolt-migration/config/tables_keys.json

# Optional: CDC Delete Handling
# CDC_DELETE_COLUMN=op
# CDC_DELETE_VALUES=D,DELETE
